<#CONCEPTSET>
    a rr:TriplesMap;
    rml:logicalSource [
    	rml:source "TC_clean/clean_terminology.xml";
    	rml:referenceFormulation ql:XPath;
        rml:iterator "/root";
    ];
    rr:subjectMap [
    	a rr:Subject ;
    	rr:termType rr:IRI ;
    	rr:template "http://w3id.org/termcat-ld/{title/clean}_ConceptSet" ;
    	rr:class ontolex:ConceptSet ;
    ];
    rr:predicateObjectMap [
    	rr:predicateMap	[ rr:constant prov:wasAttributedTo];
    	rr:objectMap	[ rml:reference "author"; rr:termType rr:Literal; rr:datatype xsd:string ]
    ];
    rr:predicateObjectMap [
    	rr:predicateMap	[ rr:constant prov:hasPrimarySource];
    	rr:objectMap	[ rml:reference "title/original"; rr:termType rr:Literal; rr:datatype xsd:string ]
    ];
.



